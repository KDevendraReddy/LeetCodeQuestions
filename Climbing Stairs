/*
70 Climbing Stairs
https://leetcode.com/problems/climbing-stairs/
*/
class Solution {
    public int climbStairs(int n) {
        int prev=1;
        if(n==1)
        return prev;
        int prev2=2;
        int curr=0;
        for(int i=3;i<=n;i++)
        {
         curr=prev+prev2;
         prev=prev2; 
         prev2=curr;  
        }
        return prev2;
    }
}
